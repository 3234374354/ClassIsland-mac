	</PropertyGroup>

	<PropertyGroup>

	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\ClassIsland.Core\ClassIsland.Core.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Folder Include="Properties\" />
	</ItemGroup>
</Project>
